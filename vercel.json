{
  "redirects": [
    { "source": "/aktuelles.htm", "destination": "/news", "permanent": true },
    { "source": "/kontakt.htm", "destination": "/kontakt", "permanent": true },
    { "source": "/impressum.htm", "destination": "/impressum", "permanent": true },
    { "source": "/datenschutz.htm", "destination": "/datenschutz", "permanent": true }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*).(jpg|jpeg|png|svg|webp|gif)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}